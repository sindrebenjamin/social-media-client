{
  "name": "css-frameworks-ca",
  "version": "2.0.2",
  "description": "Test",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "prepare": "husky install",
    "test": "npm run test-unit && cy:run",
    "test-unit": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-cypress": "^2.15.1",
    "jest": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "live-server": "^1.2.2",
    "prettier": "^3.0.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "jest": {
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
}
